{
    "env": {
      "browser": true,
      "node": true,
      "cypress/globals": true
    },
    "extends": [
      "eslint:recommended",
      "plugin:cypress/recommended",
      "plugin:@typescript-eslint/recommended",
      "prettier"
    ],
    "parser": "@typescript-eslint/parser",
    "plugins": ["cypress", "@typescript-eslint"],
    "rules": {
      "@typescript-eslint/no-unused-vars": ["warn"]
    }
  }